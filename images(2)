<script language="javascript" src="https://dct.zoosnet.net/JS/LsJS.aspx?siteid=DCT67601660&float=1&lng=cn"></script>
<script type="text/javascript">
    var siteThreeClassArr = [];
    var siteClassBrandArr = [];

    $(function () {
        childClassInfoFun();

        $(document).on('mouseenter', '.pull-box-li', function() {
            var _this = $(this);
            var cateId = _this.attr('cate-id');
            if(cateId) {
                $.ajax({
                    url: '/goods/get-class-by-id',
                    type: 'post',
                    data: {
                        cateId: cateId
                    },
                    dataType: 'json',
                    success: function(json) {
                        if(json['cache'] && json['cache'] == 1 && json['ret'] == 200) {
                            _this.find('.pull-down').html(json['data']);
                        } else {
                            siteClassStyleFun(json['data'], _this);
                        }
                    },
                    error: function(reg) {
                        console.log(reg);
                    }
                });
            }
        });
    });

    /**
     * 直接获取分类下面的子分类
     * */
    function childClassInfoFun()
    {
        var cateIds = '';
        $.each($('.pull-box-li'), function(i, obj) {
            var _this = $(this);
            var cateId = _this.attr('cate-id');
            if(cateId) {
                cateIds += cateId+',';
            }
        });

        var ids = cateIds.substring(0, cateIds.length - 1);// 截取开始到长度减-1的字符串，去掉逗号
        $.ajax({
            url: '/site/child-class-info',
            type: 'post',
            data: {
                cateId: ids,
                _csrf: 'zwrK6zYts-hL6vNb4YvJdgUggUWqVbe_xyy5uKNUDIOIPr2zG1rqgSOemyKN-K0vSRLyLvolgYeTHtfdyhY_3A=='
            },
            dataType: 'json',
            success: function(json) {},
            error: function(reg) {}
        });
    }

    /**
     * 赋予分类数据
     * */
    function siteClassStyleFun(data, me)
    {
        var html = '';

        $.each(data, function(i, obj) {
            if(i > 4) {
                return false;
            }

            var info2 = {};

            info2['cate_id'] = obj['cate_id'];
            info2['cate_name'] = obj['cate_name'];
            siteClassBrandArr[i] = info2;

            if(obj['sons'] && obj['sons'].length>0) {
                html += '<div class="classify-for">';
                var data = [];
                var tips = 0;
                $.each(obj['sons'], function(x, cla) {
                    html += '<dl>';
                    html += '   <dt><a target="_blank" title="'+cla['cate_name']+'" href="/category-'+cla['cate_code_id']+'">'+cla['cate_name']+'</a></dt>';
                    html += '   <dd>';

                    if(cla['sons'].length>0) {
                        $.each(cla['sons'], function(z, _cla) {
                            var org = '';
                            if(_cla['cate_is_highlight'] == 1) {
                                org = 'class="org"';
                            }
                            html += '<a target="_blank" title="'+_cla['cate_name']+'" '+org+' href="/category-'+_cla['cate_code_id']+'-'+_cla['cate_leve']+'">'+_cla['cate_name']+'</a>';
                        });
                    } else {
                        html += '&nbsp;';
                    }

                    html += '   </dd>';
                    html += '</dl>';
                });
                html += '</div>';

                html += '<div class="recomBrand recomBrand-'+i+'">';
                html += '   <dl class="brand">';
                html += '   </dl>';
                html += '   <dl class="activity">';
                html += '   </dl>';
                html += '</div>';
            }
        });

        me.find('.pull-down').html(html);
        giveSiteClassBrandFun();
        giveSiteClassAdFun();
    }

    /**
     * 分类下面的品牌
     * */
    function giveSiteClassBrandFun()
    {
        $.each(siteClassBrandArr, function(i, obj) {
            if(obj['cate_id']) {
                $.ajax({
                    url: '/site/ajax-class-brand',
                    type: 'post',
                    data: {
                        cateId: obj['cate_id'],
                        _csrf: 'zwrK6zYts-hL6vNb4YvJdgUggUWqVbe_xyy5uKNUDIOIPr2zG1rqgSOemyKN-K0vSRLyLvolgYeTHtfdyhY_3A=='
                    },
                    dataType: 'json',
                    success: function(json) {
                        if(json['ret'] == 200) {
                            var html = '';
                            if(json['data'] && json['data'].length>0) {
                                html += '   <dt>推荐品牌</dt>';
                                $.each(json['data'], function(z, _obj) {
                                    var logo = '/shop_pc/images/img/tiyandian.jpg';
                                    if(_obj['brand_logo']) {
                                        logo = _obj['brand_logo'];
                                    }

                                    var brandName = '';
                                    if(_obj['brand_name']) {
                                        brandName = _obj['brand_name'];
                                    }

                                    if(z < 6) {
                                        html += '<dd><a target="_blank" title="brandName" href="/search-'+brandName+'.html"><img src="'+logo+'" /></a></dd>';
                                    }

                                });
                                $('.recomBrand-'+i).find('.brand').html(html);
                            }

                        } else {
                            //layer.msg(json['msg']);
                        }
                    },
                    error: function(reg) {
                        console.log(reg);
                    }
                });
            }
        });
    }

    /**
     * 分类下面的广告
     * */
    function giveSiteClassAdFun()
    {
        $.each(siteClassBrandArr, function(i, obj) {
            if(obj['cate_id']) {
                var code = '';
                var cateId = obj['cate_id'];

                 // 正式
                 if(cateId == '4cef7c957d5a4d89926f97fb25e3a9d2') {
                 code = '10134';
                 } else if(cateId == 'f87eae197057402f9fa4afa41e4907c0') {
                 code = '10135';
                 } else if(cateId == '8f4927268851460a97781ad031822b66') {
                 code = '10136';
                 } else if(cateId == '29938d6509ea43a8acc1849b5326a942') {
                 code = '10137';
                 } else if(cateId == '1f276fee4d1742d6a55833317275e56b') {
                 code = '10138';
                 }

                $.ajax({
                    url: '/site/ajax-class-ad',
                    type: 'post',
                    async: false,
                    data: {
                        code: code,
                        _csrf: 'zwrK6zYts-hL6vNb4YvJdgUggUWqVbe_xyy5uKNUDIOIPr2zG1rqgSOemyKN-K0vSRLyLvolgYeTHtfdyhY_3A=='
                    },
                    dataType: 'json',
                    success: function(json) {
                        if(json['ret'] == 200) {
                            var html = '';
                            if(json['data']) {
                                html += '   <dt>精选活动</dt>';
                                html += '   <dd><a target="_blank" title="'+json['data']['position']['advertisingList'][0]['materialList'][0]['name']+'" href="'+json['data']['position']['advertisingList'][0]['materialList'][0]['href']+'"><img alt="'+json['data']['position']['advertisingList'][0]['materialList'][0]['name']+'" src="'+json['data']['position']['advertisingList'][0]['materialList'][0]['file_key']+'"></a></dd>';
                                $('.recomBrand-'+i).find('.activity').html(html);
                            }

                        } else {
                            //layer.msg(json['msg']);
                        }
                    },
                    error: function(reg) {
                        console.log(reg);
                    }
                });
            }
        });
    }
</script>
