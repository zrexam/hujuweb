<script type="text/javascript" src="https://api.map.baidu.com/api?v=2.0&ak=4qaHc40CpF5dp1tqqgeZ6NpRPA3VfHhf"></script>
<script type="text/javascript">
    var likeIndex = '1';

    var cityName = '' ? '' : '';

    var map = new BMap.Map("allmap");
    var geolocation = new BMap.Geolocation();
    var myCity = new BMap.LocalCity();

    if(cityName == '') {
        geolocation.getCurrentPosition(function(r){
            if(this.getStatus() == BMAP_STATUS_SUCCESS){
                //$('#lng').val(r.point.lng);
                //$('#lat').val(r.point.lat);
                myCity.get(myFun);
            }
        },{enableHighAccuracy: true});
    } else {
        recommendExpFun(cityName, '');
    }
    function myFun(result){
        var city = result.name;
        var lat = result.center.lat;
        var lng = result.center.lng;
        recommendExpFun(city, '');
        $.ajax({
            url: '/site/ajax-position-info',
            type: 'post',
            data: {
                city: city,
                lat: lat,
                lng: lng,
                _csrf: 'zwrK6zYts-hL6vNb4YvJdgUggUWqVbe_xyy5uKNUDIOIPr2zG1rqgSOemyKN-K0vSRLyLvolgYeTHtfdyhY_3A=='
            },
            dataType: 'json',
            success: function(json) {
                if(json['ret'] == 1) {
                    $('.poitionCity').text(city);
                    //console.log(json['msg']);
                } else {
                    //console.log(json['msg']);
                }
            },
            error: function(reg) {
                console.log(reg);
            }
        });
        //$('#city').val(cityName);
    }

    $(function() {
        getSeckillInfoFun();
        $("img.lazy").lazyload({placeholder : "/shop_pc/images/index/transparent.gif",effect: "fadeIn"});
    });

    /**
     * 限时秒杀
     * */
    function getSeckillInfoFun()
    {
        $.ajax({
            url: '/goods/ajax-seckill-info',
            type: 'post',
            data: {
                showType: '1',
                page: likeIndex,
                _csrf: 'zwrK6zYts-hL6vNb4YvJdgUggUWqVbe_xyy5uKNUDIOIPr2zG1rqgSOemyKN-K0vSRLyLvolgYeTHtfdyhY_3A=='
            },
            dataType: 'json',
            success: function(json) {
                if(json['ret'] == 200) {

                    var date = new Date();
                    var year = date.getFullYear();
                    var month = date.getMonth() < 10 ? "0" + date.getMonth()+1 : date.getMonth()+1;
                    var day = date.getDate() < 10 ? "0" + date.getDate() : date.getDate();
                    var hour = date.getHours() < 10 ? "0" + date.getHours() : date.getHours();
                    var minute = date.getMinutes() < 10 ? "0" + date.getMinutes() : date.getMinutes();
                    var seconds = date.getSeconds() < 10 ? "0" + date.getSeconds() : date.getSeconds();
                    var dTime = year+'-'+month+'-'+day+' '+hour+':'+minute+':'+seconds;

                    var day1 = date.getDate() < 10 ? "0" + date.getDate()+1 : date.getDate()+1;
                    var dTime1 = year+'-'+month+'-'+day1+' '+hour+':'+minute+':'+seconds;
                    if(json['data'] && json['data'].length>0 && ((dTime >= json['data'][0]['act_date_start']) && (dTime <= json['data'][0]['act_date_end']))) {
                        var html1 = '';
                        html1 += '<span class="fl">限时秒杀</span>';
                        html1 += '<div class="cd-time" data-time=""></div>';
                        html1 += '<a target="_blank" class="fr org" href="/seckill/index">更多</a>';
                        $('.recommend-brand').find('.title').html(html1);
                        seckillBoxFun(json['data'][0], 1);

                    } else if(json['data'] && json['data'].length>0 && (dTime1 >= json['data'][0]['act_date_start'])) {
                        var html1 = '';
                        html1 += '<span class="fl">限时秒杀</span>';
                        html1 += '<div class="cd-time" data-time=""></div>';
                        html1 += '<a target="_blank" class="fr org" href="/seckill/index">更多</a>';
                        $('.recommend-brand').find('.title').html(html1);
                        seckillBoxFun(json['data'][0], 2);

                    } else {
                        $('.recommend-brand').find('.title').remove('');
                        $('.recommend-brand').find('.bd').remove('');
                    }

                } else {
                    var html = '';
                    html += '距结束';
                    html += '<span class="hours">'+00+'</span> 时';
                    html += '<span class="minutes">'+00+'</span> 分';
                    html += '<span class="seconds">'+00+'</span>秒';
                    $('.recommend-brand').find('.title').remove('');
                    $('.recommend-brand').find('.bd').remove('');
                }

            },
            error: function(reg) {
                console.log(reg);
            }
        });
    }

    /**
     * 限时秒杀结构
     * */
    function seckillBoxFun(data, type)
    {
        var end_time = '';
        if(type == 1) {
            end_time = data['act_date_end'];
        } else if(type == 2) {
            end_time = data['act_date_start'];
        }
        
        data['startTime'] = data['act_date_start']; // 开始时间
        data['endTime'] = data['act_date_end']; // 结束时间
        var timestamp = new Date().getTime();
        $('.recommend-brand').find('.title .cd-time').attr({'data-time': end_time, 'diff-key': timestamp, 'type': type}).html();
        var tpl = document.getElementById('seckillInfoTmp').innerHTML;
        var html = template(tpl, {data: data});
        $('.recommend-brand').find('.bda').addClass('bd');
        $('.recommend-brand').find('.bd').removeClass('bda');
        $('.recommend-brand').find('.bd').html(html);
        $('.recommend-brand').find('.title .cd-time').setTimers();

        $(".picScroll-left").slide({
            mainCell:".bd ul",
            autoPage:true,
            effect:"leftLoop",
            autoPlay:true,
            vis:5
        });
    }

    /**
     * 推荐体验店
     * @param cityName 城市名
     * @param adcode 城市编码
     * @return mixed
     * */
    function recommendExpFun(cityName, adcode)
    {
        $.ajax({
            url: '/site/recommend-exp',
            type: 'post',
            data: {
                cityName: cityName,
                adcode: adcode,
                _csrf: 'zwrK6zYts-hL6vNb4YvJdgUggUWqVbe_xyy5uKNUDIOIPr2zG1rqgSOemyKN-K0vSRLyLvolgYeTHtfdyhY_3A=='
            },
            dataType: 'json',
            success: function(json) {
                if(json['ret'] == 200) {
                    if(json['data']['rows']) {
                        var info = json['data']['rows'];
                        var tpl = document.getElementById('recommendExp').innerHTML;
                        var data={data:info,cityName:cityName};
                        var html = template(tpl,data);
                        $('.discovery-hall').html(html);
                    }
                } else {
                    console.log(json['msg']);
                }
            },
            error: function(reg) {
                console.log(reg);
            }
        });
    }

    //时间为一位数时显示格式为："0X"
    function checkTime(i) {
        if (i < 10) {
            i = "0" + i;
        }
        return i;
    }

    (function($){
        $.fn.extend({
            setTimers: function(){
                $(this).each(function(item,i){
                    if($(this).attr('data-time')){
                        (function(dom){
                            var k = dom.attr('diff-key');
                            function timer(){
                                var intdiff = dom.attr('data-time');
                                var type = dom.attr('type');
                                var end = new Date(intdiff).getTime();
                                var timestamp = new Date().getTime();
                                var diff = (end - timestamp);
                                var d = h = m = s = 0;
                                var d = parseInt(diff / 1000 / 60 / 60 / 24 , 10); //计算剩余的天数
                                //var h = parseInt(diff / 1000 / 60 / 60 % 24 , 10); //计算剩余的小时
                                var h = parseInt(diff / 1000 / 60 / 60, 10); //计算剩余的小时
                                var m = parseInt(diff / 1000 / 60 % 60, 10);//计算剩余的分钟
                                var s = parseInt(diff / 1000 % 60, 10);//计算剩余的秒数
                                h = checkTime(h);
                                m = checkTime(m);
                                s = checkTime(s);
                                if(type == 2) {
                                    if(diff <= 0){
                                        clearInterval(k);
                                        window.location.reload();
                                    }else{
                                        var html = '';
                                        html += '距开始';
                                        html += '<span class="hours">'+h+'</span> 时';
                                        html += '<span class="minutes">'+m+'</span> 分';
                                        html += '<span class="seconds">'+s+'</span>秒';
                                        dom.html(html);
                                        //dom.html('<i></i>剩余'+h+'小时'+ m+'分'+s+'秒');
                                    }
                                } else {
                                    if(diff <= 0){
                                        clearInterval(k);
                                        getSeckillInfoFun();
                                    }else{
                                        var html = '';
                                        html += '距结束';
                                        html += '<span class="hours">'+h+'</span> 时';
                                        html += '<span class="minutes">'+m+'</span> 分';
                                        html += '<span class="seconds">'+s+'</span>秒';
                                        dom.html(html);
                                        //dom.html('<i></i>剩余'+h+'小时'+ m+'分'+s+'秒');
                                    }
                                }
                            }
                            timer();
                            var k;
                            k = setInterval(timer, 1000);
                        })($(this));
                    }
                })
            }
        });
    })(jQuery);

</script>
