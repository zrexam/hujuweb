            $(document).on('keyup', '.search-box', function(event) {
                if(event.keyCode == 13) {
                    $('#search-goods').trigger('click');
                }
            });

            function size(obj){
                obj.classList.add("org");
            }

            function size1(obj){
                obj.classList.remove("org");
            }

            /**
             * @return mixed
             * */
            function searchFun()
            {
                var search = $('#search-value').val() ? '-'+$('#search-value').val() : '';
                window.location.href = "/search" + search+".html";
            }
        </script>
